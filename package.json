{
  "name": "redyna",
  "version": "0.0.1",
  "description": "Register/unregister reducers for redux.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "directories": {
    "test": "jest"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -f cjs -o lib/index.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -f es -o es/index.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -f umd -o dist/redyna.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -f umd -o dist/redyna.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "lint": "eslint src test",
    "posttest": "npm run lint",
    "prepare": "npm run clean && npm run test && npm run build",
    "test": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kouhin/redyna.git"
  },
  "homepage": "https://github.com/kouhin/redyna",
  "keywords": [
    "redux",
    "reducer",
    "dynamic",
    "module"
  ],
  "author": "Bin Hou <houbin217jz@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-react": "^7.0.0-beta.47",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.5",
    "eslint-config-airbnb-deps": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-prettier": "^2.6.0",
    "jest": "^23.0.0-charlie.2",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.1",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
