{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "eslint-config-airbnb",
    "plugin:prettier/recommended",
    "plugin:jest/recommended"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "import",
    "babel",
    "jest"
  ],
  "rules": {
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0
  }
}
